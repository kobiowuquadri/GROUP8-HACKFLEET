{
  "extends": "airbnb-base",
  "env": {
    "node": true,
    "browser": true,
    "es6": true
  },
  "rules": {
    "no-console": "off",
    "import/no-extraneous-dependencies": ["error", {
      "devDependencies": true
    }],
    "max-len": ["error", {
      "code": 120,
      "ignoreComments": true,
      "ignoreUrls": true
    }],
    "no-underscore-dangle": "off",
    "no-param-reassign": ["error", {
      "props": false
    }]
  }
} 